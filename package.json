{
	"name": "react-loading-indicators",
	"version": "0.1.1",
	"description": "Simply elegant light-weight loading indicators for react applications",
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "npm run build:cjs && npm run build:mjs && npm run packTypes",
		"build:cjs": "node scripts/builder.cjs --target commonjs --mode production",
		"build:mjs": "node scripts/builder.cjs --target module --mode production",
		"packTypes": "node scripts/typesPack.cjs",
		"checkVersion": "node scripts/versionExistsCheck.cjs",
		"test": "echo 'no tests yet'",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"prepublishOnly": "npm run build && npm run test",
		"format": "prettier --ignore-path .gitignore --write \"src/**/*.ts\" \"src/**/*.js\""
	},
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.21.3",
		"@babel/preset-react": "^7.18.6",
		"@storybook/addon-actions": "^6.5.16",
		"@storybook/addon-essentials": "^6.5.16",
		"@storybook/addon-interactions": "^6.5.16",
		"@storybook/addon-links": "^6.5.16",
		"@storybook/builder-webpack4": "^6.5.16",
		"@storybook/manager-webpack4": "^6.5.16",
		"@storybook/react": "^6.5.16",
		"@storybook/testing-library": "^0.0.13",
		"@types/react": "^18.0.28",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^8.3.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"dts-bundle": "^0.7.3",
		"fork-ts-checker-webpack-plugin": "^8.0.0",
		"postcss": "^8.4.21",
		"postcss-loader": "^7.1.0",
		"prettier": "^2.8.6",
		"react": "^16.8.0",
		"react-dom": "^16.8.0",
		"sass": "^1.59.3",
		"sass-loader": "^13.2.1",
		"style-loader": "^3.3.2",
		"terser-webpack-plugin": "^5.3.7",
		"ts-loader": "^9.4.2",
		"typescript": "^5.0.2",
		"webpack": "^5.76.2",
		"webpack-cli": "^5.0.1",
		"webpack-merge": "^5.8.0",
		"yargs": "^17.7.1"
	},
	"main": "./dist/cjs/index.js",
	"module": "./dist/mjs/index.js",
	"exports": "./dist/mjs/",
	"files": [
		"dist"
	],
	"sideEffects": [
		"./src/**/*.{scss, sass}"
	],
	"types": "dist/index.d.ts",
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"bin": {
		"builder": "./scripts/builder.cjs"
	},
	"keywords": [
		"react",
		"react-loading",
		"react-spinners",
		"react-loading-indicators",
		"loading",
		"spinners",
		"typescript",
		"css loading",
		"loaders"
	],
	"author": {
		"name": "zacky",
		"email": "hanesmitter3@gmail.com",
		"url": "https://lookupzach.netlify.app"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hane-smitter/react-loading-indicator"
	},
	"homepage": "https://react-loading-indicators.netlify.app/",
	"dependencies": {
		"colortranslator": "^1.10.2"
	}
}
