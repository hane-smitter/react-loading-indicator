{
  "name": "react-loading-indicators",
  "version": "0.2.3",
  "description": "Simply elegant light-weight loading indicators for react applications",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "postbuild": "rm -rf dist/esm/types && rm -rf dist/cjs/types",
    "checkVersion": "node scripts/versionExistsCheck.cjs",
    "test": "echo 'no tests yet'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build && npm run test",
    "format": "prettier --ignore-path .gitignore --write \"src/**/*.ts\" \"src/**/*.js\""
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@mdx-js/react": "^3.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-actions": "^7.5.1",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-webpack5": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.6",
    "@types/react": "^18.2.33",
    "@types/tinycolor2": "^1.4.5",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "chalk": "4.1.2",
    "css-loader": "^6.8.1",
    "glob": "^10.3.10",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.1.5",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "storybook": "^7.5.1",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./src/**/*.{scss, sass}"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
